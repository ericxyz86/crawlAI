# Environment variables for Web Scraper App
# Copy this file to .env and update with your values

# App settings
APP_ENV=development
LOG_LEVEL=INFO

# Crawl4AI settings
CRAWL4AI_HEADLESS=true
CRAWL4AI_BROWSER_TYPE=chromium
CRAWL4AI_DEFAULT_TIMEOUT=30000
CRAWL4AI_DEFAULT_WAIT=3000

# Server settings
HOST=0.0.0.0
PORT=8000